<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportListController">
   <children>
      <TableView fx:id="reportTable" stylesheets="@CSS/Styles.css">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="dateCol" prefWidth="75.0" text="Date" />
          <TableColumn fx:id="creatorCol" prefWidth="70.0" text="Creator" />
            <TableColumn fx:id="latitudeCol" prefWidth="46.0" text="Latitude" />
            <TableColumn fx:id="longitudeCol" prefWidth="56.0" text="Longitude" />
            <TableColumn fx:id="typeCol" minWidth="9.0" prefWidth="87.0" text="Type" />
            <TableColumn fx:id="conditionCol" minWidth="0.0" prefWidth="98.0" text="Condition" />
            <TableColumn fx:id="descriptionCol" maxWidth="167.0" minWidth="0.0" prefWidth="167.0" text="Description" />
        </columns>
      </TableView>
   </children>
</StackPane>
